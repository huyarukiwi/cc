/* pages/CreateAddress/CreateAddress.wxss */
.CreateAddressList{
  padding:43rpx 20rpx 46rpx;
  margin:  0 40rpx;
  height: 48rpx;
  display: flex;
  border-bottom: 1px solid #CBCBCB;
}
.CreateAddressListTit{
  width: 190rpx;
  display: inline-block;
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #999999;
  letter-spacing: 0;
}
.CreateAddressList input {
  padding-left: 12rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  letter-spacing: 0;
}
.Provinces{
  padding-left: 12rpx;
  width: 40rpx;
  height: 51rpx;
}
.section{
  width: 390rpx;
  height: 48rpx;
  line-height: 48rpx;
  display: inline-block;
}
.picker{
  padding-left: 12rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  letter-spacing: 0;
}
.phcolor{
  font-size: 34rpx;
  color: #CBCBCB;
}
.DataBtns{
  position: fixed;
  bottom: 20rpx;
}
.DataBtns button{
  width: 344rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 8rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  letter-spacing: 0;
}
.DataBtns .DataBtnsRemove{
  background: #FFFFFF;
  border: 1px solid #CBCBCB;
  color: #999999;
}
.DataBtns .DataBtnsRemove{
  border: 1px solid #CBCBCB;
  margin-left: 20rpx;
  background: #FFFFFF;
  margin-right: 22rpx;
  }
.DataBtns .DataBtnsSave{
  border: 1px solid #CBCBCB;
  color: #FFFFFF;
  background: #409EFF;
}
